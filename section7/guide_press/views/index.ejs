<%- include("./partials/header.ejs") %>
<%- include("./partials/homenavbar.ejs") %>
<body>
    <div class="container">
        <hr>
        <div class="jumbotron">
            <h1 class="display-4">Hello, developers!</h1>
            <p class="lead">This is a blog to share information about the world of technology.</p>
            <hr class="my-4">
            <p>Always stay well informed through the articles published on our blog.</p>
            <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
        </div>
        <hr>
        <% articles.forEach(article => { %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title %></h2>
                </div>
                <div class="card-body">
                    <a href="#" class="btn btn-success">Read more</a>
                </div>
            </div>
            <br>
        <% }) %>
    </div>
</body>
<%- include("./partials/footer.ejs") %>